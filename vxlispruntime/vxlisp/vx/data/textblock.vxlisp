            :startpos 2
            :endpos 13
            :delim
             (copy delimbracketcurly
              :delimlist delimlisttest2)
            :close
             (copy delimclose
              :starttext ">")
            :parent
             (textblock
              :text "<{a,b},{c d}>"
              :startpos 1
              :endpos 13
              :delim
               (copy delimbracketangle
                :delimlist delimlisttest1)
              :parent
               (textblock
                :text "<{a,b},{c d}>"
                :startpos 1
                :endpos 13))
            :children
             (textblocklist
              (textblock
               :text "a"
               :startpos 3
               :endpos 3)
              (textblock
               :text ","
               :startpos 4
               :endpos 4
               :delim
                (copy delimcomma
